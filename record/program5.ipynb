   "source": [
    "def isPrime(n):\n",
    "    if n <= 1:\n",
    "        return False\n",
    "\n",
    "    # Check divisibility from 2 to n-1\n",
    "    for i in range(2, n):\n",
    "        if n % i == 0:\n",
    "            return False\n",
    "\n",
    "    return True\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "  n = 7\n",
    "  if(isPrime(n)): \n",
    "    print(\"true\")\n",
    "  else:\n",
    "    print(\"false\")"
   ]
