   "source": [
    "# Find the sum of digits of a number (while loop)\n",
    "num = int(input(\"Enter a number:\"))\n",
    "sm = 0\n",
    "temp = num\n",
    "while temp > 0:\n",
    "    sm += temp % 10\n",
    "    temp //= 10\n",
    "\n",
    "print(\"sum of\", num ,\"is\", sm)\n"
   ]
  }
