   "source": [
    "# Find sum of odd numbers up to a range (for loop)\n",
    "num = int(input(\"Enter the number: \"))\n",
    "sm = 0\n",
    "for i in range(1,num+1):\n",
    "    if i%2 != 0:\n",
    "        sm+=i\n",
    "print(\"The sum of odd numbers is\", sm)"
   ]
